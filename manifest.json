{
  "manifest_version": 2,
  "name": "Paste Monta",
  "version": "0.0.3",
  "permissions": [
    "storage",
    "background",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"],
      "css": ["embeded.css"],
      "run_at" : "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "embeded.js",
    "embeded.css"
  ]
}
